{
  "name": "fylo-data-storage-component-master",
  "version": "1.0.0",
  "description": "flyo-data",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/flyo-data-storage/main.scss css/flyo-style.css -w",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/flyo-data-storage/main.scss css/fylo-style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 version\" css/fylo-style.comp.css -o css/flyo-style.prefix.css",
    "compress:css": "node-sass css/flyo-style.prefix.css css/flyo-style.css --output-style compressed",
    "build:css": "npm-run-all compile:sass  prefix:css compress:css"
  },
  "author": "Gaurav Yadav",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.8.0",
    "concat": "^1.0.3",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1"
  }
}
